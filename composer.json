{
    "name": "alsciende/behat-json-context",
    "description": "Context to help test JSON with Behat",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alsciende",
            "email": "alsciende@icloud.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Alsciende\\Behat\\": "src/"
        }
    },
    "require": {
        "php": ">=7.1.0",
        "beberlei/assert": "^2.9",
        "behat/behat": "^3.4",
        "justinrainbow/json-schema": "^5.2",
        "symfony/property-access": "^4.1"
    },
    "suggest": {
        "alsciende/behat-api-context": "Allows for importing data from API responses"
    },
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2"
    }
}
